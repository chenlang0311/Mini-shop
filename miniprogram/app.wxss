.sel_dot{
  width: 36rpx;
  height: 36rpx;
  position: relative;
  margin-right: 32rpx;
}

.sel_dot::before{
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  border: 1px solid #ccc; 
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: left top;
  transform-origin: left top;
  pointer-events: none; 
  border-radius: 50%; 
}


view{
  box-sizing: border-box;
}
button {
  background: initial;
  box-sizing: border-box;
  padding: 0;
  border-radius: 0;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}

image{
  display: block;
}
.c_red{
  color: #ff5146;
}
.fwb{
  font-weight: bold;
}
.one_bd::before{
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
    box-sizing: border-box;
    border-bottom:1px solid #f5f5f5;
    width: 200%;
    height: 200%;
    transform: scale(.5);
    transform-origin: left top;
    pointer-events: none;
}


